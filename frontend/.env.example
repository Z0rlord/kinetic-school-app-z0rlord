# Frontend Environment Configuration

# API Configuration
VITE_API_URL=http://localhost:3001/api

# Application Configuration
VITE_APP_NAME=Student Profile System
VITE_APP_VERSION=1.0.0

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG=false

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/jpeg,image/png

# Feature Flags
VITE_ENABLE_RESUME_PARSING=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_SURVEYS=true
